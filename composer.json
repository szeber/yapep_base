{
  "name": "szeber/yapep_base",
  "type": "library",
  "description": "YAPEP Base framework",
  "require": {
    "php": ">=7.3.0",
    "lukasoppermann/http-status": "^2.0.1",
    "psr/container": "1.0.0",
    "pimple/pimple": "^3.2",
    "emulgeator/server-superglobal-object-representation": "1.0.3",
    "ext-memcached": "*",
    "ext-json": "*",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.2",
    "friendsofphp/php-cs-fixer": "^2.14.2",
    "ext-xdebug": "*",
    "symfony/yaml": "^4.2.3"
  },
  "suggest": {
    "symfony/yaml": "Required for working with YAML files"
  },
  "autoload": {
    "psr-4": {
      "YapepBase\\": "src",
      "YapepBase\\Test\\": "test"
    }
  },
  "authors": [
    {
      "name": "Zsolt Szeberenyi",
      "email": "szeber@yapep.org"
    },
    {
      "name": "László Fodor (emul)"
    }
  ]
}
